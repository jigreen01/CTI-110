speed = int(input('Enter car speed: '))        
time = int(input('Enter time: '))   

if time < 1:  
 time = 1
     

#Calculate distance traveled
distance = speed * time    

# Display speed, time and distance
print('Speed Entered: ', speed)
print('Time Entered: ', time)
print('Distance traveled: ', distance)